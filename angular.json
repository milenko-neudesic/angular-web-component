{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "angular-web-component",
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/angular"
  },
  "projects": {
    "angular-web-component": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/angular-web-component",
      "sourceRoot": "apps/angular-web-component/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-web-component",
            "index": "apps/angular-web-component/src/index.html",
            "main": "apps/angular-web-component/src/main.ts",
            "polyfills": "apps/angular-web-component/src/polyfills.ts",
            "tsConfig": "apps/angular-web-component/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/angular-web-component/src/favicon.ico",
              "apps/angular-web-component/src/assets"
            ],
            "styles": [
              "apps/angular-web-component/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-web-component/src/environments/environment.ts",
                  "with": "apps/angular-web-component/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-web-component:build",
            "sslKey": "localhost.key",
            "sslCert": "localhost.crt"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-web-component:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-web-component:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/angular-web-component/src/test.ts",
            "polyfills": "apps/angular-web-component/src/polyfills.ts",
            "tsConfig": "apps/angular-web-component/tsconfig.spec.json",
            "karmaConfig": "apps/angular-web-component/karma.conf.js",
            "assets": [
              "apps/angular-web-component/src/favicon.ico",
              "apps/angular-web-component/src/assets"
            ],
            "styles": [
              "apps/angular-web-component/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-web-component/tsconfig.app.json",
              "apps/angular-web-component/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "angular-web-component:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-web-component:serve:production"
            }
          }
        }
      }
    },
    "angular-web-component-e2e": {
      "root": "apps/angular-web-component-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/angular-web-component-e2e/protractor.conf.js",
            "devServerTarget": "angular-web-component:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-web-component:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/angular-web-component-e2e/tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  }
}